{
  "name": "react-redux-starter-kit",
  "version": "1.0.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "build:i18n": "rm -rf _translations && BABEL_ENV=i18n babel --quiet src > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node --babelrc=false scripts/i18nToXliff.js",
    "manage:translations": "babel-node-debug ./bin/translationRunner.js"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "formsy-material-ui": "^0.3.8",
    "formsy-react": "^0.18.0",
    "history": "^2.0.0",
    "humps": "^1.0.0",
    "intl": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.5.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.5.1",
    "material-ui": "^0.14.2",
    "normalizr": "^2.0.0",
    "react": "^0.14.0",
    "react-data-grid": "^0.14.22",
    "react-dom": "^0.14.0",
    "react-intl": "^2.0.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0-beta",
    "react-select": "^1.0.0-beta12",
    "react-swipeable-views": "^0.4.2",
    "react-tap-event-plugin": "^0.2.1",
    "react-title-component": "^1.0.1",
    "react-typeahead": "^1.1.7",
    "redux": "^3.0.0",
    "redux-actions": "^0.9.0",
    "redux-auth-wrapper": "^0.3.0",
    "redux-form": "^5.2.3",
    "redux-thunk": "^1.0.0",
    "validatorjs": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-intl": "^2.0.0",
